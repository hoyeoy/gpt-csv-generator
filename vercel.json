{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.9",
      "maxDuration": 60,
      "regions": ["icn1"]   
    }
  },
  "routes": [
    { "src": "/api/startuprecipe", "dest": "api/index3.py" },
    { "src": "/api/thebell", "dest": "api/index2.py" },
    { "src": "/api/(.*)", "dest": "api/index.py" }
  ]
}